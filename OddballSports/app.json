{
  "expo": {
    "name": "OddballSports",
    "slug": "OddballSports",
    "privacy": "public",
    "owner": "oddballsports",
    "description": "Testing app for ABC Class Ratings",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.drhoffma.OddballSports",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.drhoffma.OddballSports"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
